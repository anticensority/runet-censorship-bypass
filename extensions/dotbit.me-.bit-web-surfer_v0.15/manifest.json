{
"update_url": "https://clients2.google.com/service/update2/crx",

	"background": {
		"scripts": [ "proxy.js" ]
	},
	"icons": {
		"128": "icon-128.png"
	},
	"manifest_version": 2,
	"name": "dotbit.me .bit web surfer",
